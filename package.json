{
  "name": "jsore",
  "version": "0.2.0",
  "description": "temp splash page",
  "main": "index.js",
  "scripts": {
    "monitor": "pm2 monit",
    "serve:fresh": "pm2 start index.js --name 'jsore'",
    "serve:overwrite": "yarn run serve:stop && pm2 delete jsore && pm2 start index.js --name 'jsore'",
    "serve:stop": "pm2 stop jsore",
    "watch:existing": "nodemon --watch dist/index.html --watch dist/styles.css --watch index.js --exec yarn run serve:overwrite"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com-jsore:jsore/jsore.git"
  },
  "author": "Justin Sorensen <justin@jsore.com> (https://jsore.com)",
  "license": "ISC",
  "nodemonConfig": {
    "ignore": ["node_modules/*"]
  },
  "devDependencies": {
    "morgan": "^1.9.1",
    "nodemon": "^1.19.1",
    "pm2": "^3.5.1"
  },
  "dependencies": {
    "express": "^4.17.1",
    "jsonfile": "^5.0.0"
  }
}
